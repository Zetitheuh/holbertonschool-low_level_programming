#include <stdlib.h>
#include <time.h>
#include <stdio.h>

#include <stdlib.h>
#include <time.h>
#include <stdio.h>

int main(void)
{
    int n = 98; // Définition explicite de n à 98 pour le test
    int last_number;

    last_number = n % 10; // Obtient le dernier chiffre de n

    if (last_number == 0)
    {
        printf("Last digit of %d is %d and is 0\n", n, last_number);
    }
    else if (last_number < 6)
    {
        printf("Last digit of %d is %d and is less than 6 and not 0\n", n, last_number);
    }
    else // last_number > 5
    {
        printf("Last digit of %d is %d and is greater than 5\n", n, last_number);
    }

    return 0;
}

